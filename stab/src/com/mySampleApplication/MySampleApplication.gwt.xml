<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="MySampleApplication">

    <!-- Inherit the core Web Toolkit stuff. базовая функциональность  GWT фреймверка                  -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Inherit the default GWT style sheet. Обеспечивает стили для GUI компонентов, приложения -->
    <inherits name='com.google.gwt.user.theme.clean.Clean'/>

    <inherits name ='com.google.gwt.user.cellview.CellView' />

    <!-- Specify the app entry point class. объявление точки входа                   -->
    <entry-point class='com.mySampleApplication.client.MySampleApplication'/>


</module>